{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyPutObjectWithoutOwnerTinaTag",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::BUCKET_NAME/*",
      "Condition": {
        "StringNotEquals": { "s3:RequestObjectTag/Owner": "tina" }
      }
    },
    {
      "Sid": "DenyPutObjectIfTagMissing",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::BUCKET_NAME/*",
      "Condition": {
        "Null": { "s3:RequestObjectTag/Owner": "true" }
      }
    }
  ]
}
