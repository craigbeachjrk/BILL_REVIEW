{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListBucket",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": "arn:aws:s3:::jrk-analytics-billing",
      "Condition": { "StringLike": { "s3:prefix": ["Bill_Parser_4_Enriched_Outputs/*"] } }
    },
    {
      "Sid": "ReadObjects",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": "arn:aws:s3:::jrk-analytics-billing/Bill_Parser_4_Enriched_Outputs/*"
    }
  ]
}
